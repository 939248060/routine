<view><block a:for="{{list}}" a:for-item="item" a:for-index="index" a:key="index"><view class="list bg-white p10 mb10"><view class="pb10 btmccc" data-idx="{{index}}" data-event-opts="{{[['tap',[['tapAction',['$event']]]]]}}" onTap="__e"><view class="pb5">{{item.contact+" - "+item.tel}}</view><view>{{item.area+" "+item.address}}</view></view><view class="row jcbetween aicenter pt5"><block a:if="{{item.isDefault=='1'}}"><view><icon class="rout icon-dui mr5 blue1"></icon>默认</view></block><block a:else><view data-id="{{item.addressId}}" data-event-opts="{{[['tap',[['tapSetDefault',['$event']]]]]}}" onTap="__e"><icon class="rout icon-dui mr5"></icon>设为默认</view></block><view class="row jcbetween aicenter"><navigator url="{{'../addressform/addressform?id='+item.addressId}}"><icon class="rout icon-bianji ico mr5"></icon>编辑</navigator><view data-delid="{{item.addressId}}" data-event-opts="{{[['tap',[['tapRemove',['$event']]]]]}}" onTap="__e"><icon class="rout icon-shanchu ico ml10 mr5"></icon>删除</view></view></view></view></block><navigator class="foot btn f18" url="../addressform/addressform">新增地址</navigator><loading vue-id="1" onVueInit="__l"></loading><dialogs vue-id="2" id="dialog" title=" " content="是否删除地址" cancelText="取消" confirmText="确定" ref="__r" data-ref="dialog" data-event-opts="{{[['^cancelEvent',[['_cancelEvent']]],['^confirmEvent',[['_confirmEvent']]]]}}" onCancelEvent="__e" onConfirmEvent="__e" onVueInit="__l"></dialogs></view>