<view><block a:if="{{finish==false}}"><view><navigator class="viewRow address" url="../addresslist/addresslist?parent=exchange"><view><icon class="rout icon-dizhi f20"></icon></view><block a:if="{{address!=''}}"><view><view>{{address.contact+" - "+address.tel}}</view><view>{{address.area+address.address}}</view></view></block><block a:if="{{address==''}}"><view class="noadd">请选择收货地址</view></block><view><icon class="rout icon-dayuhao"></icon></view></navigator><view class="viewRow wares"><view><image class="img" src="{{host+wares.photoPath}}" mode="widthFix"></image></view><view class="viewRow"><view class="f16 black">{{"兑换 "+wares.waresName}}</view><view class="viewRow"><view>量心币：<text class="orange">{{wares.score}}</text>币</view><view>{{"x"+num}}</view></view></view><view class="viewRow"><view>合计：</view><view><text class="orange">{{wares.score*num}}</text>币</view></view></view><view><button data-event-opts="{{[['tap',[['exchange',['$event']]]]]}}" class="btn" onTap="__e">提交订单</button></view></view></block><block a:if="{{finish==true}}"><view class="finish"><view><icon class="rout icon-chenggong blue1"></icon></view><view class="mt20 mb20 f24">订单已提交</view><navigator class="nav mt15 mb15 p10" url="../personal/personal" open-type="switchTab">前往个人中心</navigator><navigator class="nav mt15 mb15 p10" url="../index/index" open-type="switchTab">返回首页</navigator></view></block></view>