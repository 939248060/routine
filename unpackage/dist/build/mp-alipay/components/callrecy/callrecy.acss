.call-mask{position:fixed;z-index:1000;top:0;right:0;left:0;bottom:0;background:rgba(0,0,0,.7)}.call-dialog{position:fixed;z-index:5000;width:90%;max-width:600rpx;top:50%;left:50%;-webkit-transform:translate(-50%,-50%);transform:translate(-50%,-50%);background-color:#fff;text-align:center;border-radius:20px;box-shadow:0 0 10px #666}.call-dialog-title{font-size:18px;padding:15px 15px 5px}.call-dialog-content{padding:15px;min-height:40px;font-size:16px;line-height:1.8;word-wrap:break-word;word-break:break-all;color:#999}.call-dialog-nothing{padding:15px;min-height:30px;font-size:16px;line-height:1.4;color:#999}.call-dialog .btn{font-size:20px;border-radius:20px;margin-bottom:10px;width:68%;line-height:40px;color:#fff;box-shadow:0 0 10px #56ab2f;background:#56ab2f;background:-webkit-linear-gradient(0deg,#56ab2f,#a8e063);background:-webkit-linear-gradient(bottom,#56ab2f,#a8e063);background:linear-gradient(0deg,#56ab2f,#a8e063)}.call-dialog .close{position:absolute;right:0;margin-right:20rpx}